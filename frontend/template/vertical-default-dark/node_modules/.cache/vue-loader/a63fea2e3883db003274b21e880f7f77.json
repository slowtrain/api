,\n        gaugeWidthScale: 0.6,\n        counter: true,\n        hideInnerShadow: true,\n        valueFontColor: \"#fff\"\n      }\n    };\n  },\n  mounted() {\n    this.$refs.g2.draw({\n      id: \"g2\",\n      value: 100,\n      title: \"refresh\",\n      defaults: this.dflt\n    });\n  },\n  methods: {\n    refresh() {\n      this.$refs.g2.refresh(this.getRandomInt(0, 100));\n    }\n  }\n};\n</script>\n"]}]}